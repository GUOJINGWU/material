	<script th:inline="javascript">
	$(function(){
	    var firstRankTypes = [[${firstRankTypes}]];//取值  
	    
	    if(typeof(firstRankTypes)!="undefined"){
		    var firstType = $("#first-type");
		    firstRankTypes.forEach(function( val, index ) {
		    	firstType.append('<option value="'+val.id+'" code="'+val.code+'">' + val.name + '</option>');
			});
	    }
	});
	</script>

	<script type="text/javascript">
		$(function(){
			/* 图片上传 */
			$("#addFile").change(function () {
                var siteImgPicCount = 0;
				$(".site-img-pic").each(function(){
					siteImgPicCount = siteImgPicCount + 1;
				});
				if(siteImgPicCount >= 5){
					AlertErrorMessage("最多可上传5张图片！");
					return false;
				}
				
				// 如果value不为空，调用文件加载方法
				if($(this).val() != ""){
					fileLoad(this);
				}
			})
			
			function fileLoad(ele){
				// 创建一个formData对象
                var formData = new FormData();
                // 获取传入元素的val
                var name = $(ele).val();
				// 获取files
                var files = $(ele)[0].files[0];
                // 将name 和 files 添加到formData中，键值对形式
                formData.append("file", files);
                formData.append("name", name);
                $.ajax({
                    url: "/web/file/upload",
                    type: 'POST',
                    data: formData,
                    processData: false,// 告诉jQuery不要去处理发送的数据
                    contentType: false,// 告诉jQuery不要去设置Content-Type请求头
                    beforeSend: function () {
						// 发送之前的动作
                    }
					,success: function (data) {
						// 成功后的动作
						var li = ''+
						'<li class="site-img site-img-pic">'+
							'<input type="hidden" name="imgNames" value="'+ data.attRelaId +'"/>'+
							'<input type="hidden" name="imgValues" value="'+ data.fileId +'"/>'+
							'<img class="li-pic" src="'+ data.fileShowUrl +'"></img>'+
							'<i class="icon" style="display: none;"></i>'+
						'</li>';
						
						$(".site-img-add").before(li);
						
						//显示删除图标
						$(".site-img").on('mouseover', function () {
						    $(this).find(".icon").show();
						});
						//隐藏删除图标
						$(".site-img").on('mouseleave', function () {
						    $(this).find(".icon").hide();
						});
						// 删除功能
						$(".site-img").on('click', '.icon', function () {
						    $(this).parent().remove();
						});
						//点击查看大图
						$(".li-pic").click(function () {
							watchBigPic($(this).attr("src"));
						});
                    }
                    ,error : function (data) {
                        // 出错后的动作
                    	AlertErrorMessage("请稍后再试");
                    }
                });
            }
			
			function isEmptyString(str){
				var bool = false;
				if(typeof(str)=="undefined" || str==null || str=='null' || str==''){
					bool = true;
				}
				return bool;
			}
			
			$.fn.toJSON = function() {
				var o = {};
				var a = this.serializeArray();
				$.each(a, function() {
					var name = this.name;
					var value = this.value;
					var paths = this.name.split(".");
					var len = paths.length;
					var obj = o;
					$.each(paths, function(i, e) {
						if (i == len - 1) {
							if (obj[e]) {
								if (!obj[e].push) {
									obj[e] = [ obj[e] ];
								}
								obj[e].push(value || '');
							} else {
								obj[e] = value || '';
							}
						} else {
							if (!obj[e]) {
								obj[e] = {};
							}
						}
						obj = o[e];
					});
				});
				return o;
			};
		
		$(".select-type-input").focus(function(){
			  $(".select-type-area").show();
		});
		
		layui.use(['form', 'layedit', 'laydate'], function(){
				var form = layui.form()
				,layer = layui.layer
				,layedit = layui.layedit
				,laydate = layui.laydate;

				//一级菜单选择
				form.on('select(first-type)', function(data){
					var secondType = $("#second-type");
					secondType.empty();
					secondType.append('<option value="">请选择二级分类</option>');
					
					var thirdType = $("#third-type");
					thirdType.empty();
					thirdType.append('<option value="">请选择三级分类</option>');
					
					// 加载二级类型列表
					var firstRankId = data.value;
					var datas = {pid : firstRankId};
					$.ajax({
						url : '/web/goodstype/subtypeList',
						type : 'POST',
						contentType: 'application/json; charset=UTF-8',
						datType: "JSON",
						data : JSON.stringify(datas),
						async : false,
						success : function(data) {
							var secondType = $("#second-type");
							secondType.empty();
							secondType.append('<option value="">请选择二级分类</option>');
							
							data.forEach(function( val, index ) {
						    	secondType.append('<option value="'+val.id+'" code="'+val.code+'">' + val.name + '</option>');
							});
						},
						error : function(data) {
							AlertErrorMessage("请稍后再试");
						}
					});  
					
					$('#second-type').attr("disabled",false);
					form.render('select'); 
				});
				 	
				//二级菜单选择
				form.on('select(second-type)', function(data){
					// 加载三级类型列表
					var secondRankId = data.value;
					var datas = {pid : secondRankId};
					$.ajax({
						url : '/web/goodstype/subtypeList',
						type : 'POST',
						contentType: 'application/json; charset=UTF-8',
						datType: "JSON",
						data : JSON.stringify(datas),
						async : false,
						success : function(data) {
							var thirdType = $("#third-type");
							thirdType.empty();
							thirdType.append('<option value="">请选择三级分类</option>');
							
							data.forEach(function( val, index ) {
				 		    	thirdType.append('<option value="'+val.id+'" code="'+val.code+'">' + val.name + '</option>');
							});
						},
						error : function(data) {
							AlertErrorMessage("请稍后再试");
						}
					}); 
				
					$('#third-type').attr("disabled",false);
					form.render('select');
				});
				
				//三级菜单选择
				form.on('select(third-type)', function(data){
					// 展示
					var textSelect1 = $("#first-type").find("option:selected").text();
					var textSelect2 = $("#second-type").find("option:selected").text();
					var textSelect3 = $("#third-type").find("option:selected").text();
					var textTotal = textSelect1+">>"+textSelect2+">>"+textSelect3;
					$(".select-type-input").val(textTotal);
					
					// 传输值
					var valSelect3 = $("#third-type").val();
					$("#tid").val(valSelect3);
					
					// 编码
					var codeSelect1 = $("#first-type").find("option:selected").attr("code");
					var codeSelect2 = $("#second-type").find("option:selected").attr("code");
					var codeSelect3 = $("#third-type").find("option:selected").attr("code");
					$("#first-type-code").val(codeSelect1);
					$("#second-type-code").val(codeSelect2);
					$("#third-type-code").val(codeSelect3);
					
					var datas = {pid : valSelect3};
					$.ajax({
						url : '/web/goods/nextGoodsCode',
						type : 'POST',
						contentType: 'application/json',
						datType: "JSON",
						data : JSON.stringify(datas),
						async : false,
						success : function(data) {
							$("#code").val(data.code);
							var goodsCodeText = codeSelect1 + codeSelect2 + codeSelect3 + data.code;
							$("#goodsCode").text(goodsCodeText);
							$("#goodsCode").removeClass();
						},
						error : function(data) {
							AlertErrorMessage("请稍后再试");
						}
					});
					
					
					$(".select-type-area").hide();
					$('#second-type').attr("disabled",true);
					form.render('select');
					$('#third-type').attr("disabled",true);
					form.render('select');

				});  
			  
		});
		
		
		//点击空白处隐藏选择
		$(".type-choose").click(function(event){
	        var e=window.event || event;
	        if(e.stopPropagation){
	            e.stopPropagation();
	        }else{
	            e.cancelBubble = true;
	        }
	    });
	    document.onclick = function(){
	        $(".select-type-area").hide();
	    };

		
		//点击查看大图
		$(".li-pic").click(function () {
			watchBigPic($(this).attr("src"));
		});
		//点击确定按钮
		$(".material-sure-submit").click(function () {
			var approveStatus = $(this).attr("approveStatus");
 			alertSubmit(approveStatus);
		});
		
		//点击保存按钮
		$(".material-margin-btn").click(function () {
			var approveStatus = $(this).attr("approveStatus");
 			alertSubmit(approveStatus);
		});
		
		/*
		* 添加物资提交
		*/
		function alertSubmit(approveStatus) {
			var txt = '<h3 class="alert-title" style="text-align: center;font-size:18px;margin-top:75px">确定提交物资信息？</h3>';
			confirmDialog("确认提交", txt, function() {
				
				//验证属性 supplyNamesCla
				if(lackSupplyParams()){
					AlertErrorMessage("请完善属性信息！");
					return false;
				}
				
				var datas = $("#add-material-form").toJSON();
				
				// 属性名
				var sNames = [];
				var supplyNames = datas.supplyNames;
				if($.isArray(supplyNames)){
					supplyNames.forEach(function(val, index) {
						if(!isEmptyString(val)){
							sNames.push(val);
		 		    	}
					});
				}else{					
					if(!isEmptyString(supplyNames)){
						sNames.push(supplyNames);
					}
				}
				datas.supplyNames = sNames;
				
				// 属性值
				var sValues = [];
				var supplyValues = datas.supplyValues;
				if($.isArray(supplyValues)){
					supplyValues.forEach(function(val, index) {
						if(!isEmptyString(val)){
							sValues.push(val);
		 		    	}
					});
				}else{					
					if(!isEmptyString(supplyValues)){
						sValues.push(supplyValues);
					}
				}
				datas.supplyValues = sValues;
				
				// 图片attRelaId
				var iNames = [];
				var imgNames = datas.imgNames;
				if($.isArray(imgNames)){
					imgNames.forEach(function(val, index) {
						if(!isEmptyString(val)){
							iNames.push(val);
		 		    	}
					});
				}else{					
					if(!isEmptyString(imgNames)){
						iNames.push(imgNames);
					}
				}
				datas.imgNames = iNames;
				
				// 图片fileId
				var iValues = [];
				var imgValues = datas.imgValues;
				if($.isArray(imgValues)){
					imgValues.forEach(function(val, index) {
						if(!isEmptyString(val)){
							iValues.push(val);
		 		    	}
					});
				}else{					
					if(!isEmptyString(imgValues)){
						iValues.push(imgValues);
					}
				}
				datas.imgValues = iValues;
				
				datas.approveStatus = approveStatus;

				$.ajax({
					url : '/web/goods/addmaterial',
					type : 'POST',
					contentType: 'application/json',
					datType: "JSON",
					data : JSON.stringify(datas),
					async : false,
					success : function(data) {
						if(data.errCode == 1000){
							if(approveStatus == 21){
								//修改成功，跳转至查看页
								window.location.href = "/web/goods/viewmaterial?gid="+data.newGid;
							}else if(approveStatus == 11){
								AlertSucMessage("物资保存成功！");
							}
							
							if(data.newGoodsId > 0){
								$("#newGoodsId").val(data.newGoodsId);
							}
						}else if(data.errCode == 30001){
							AlertErrorMessage("请完善页面参数");
						}else if(data.errCode == 30103){
							//弹窗提醒
							AlertErrorMessage(data.errMsg);
							//获取最新物资编码
							var datas = {pid : $("#tid").val()};
							$.ajax({
								url : '/web/goods/nextGoodsCode',
								type : 'POST',
								contentType: 'application/json',
								datType: "JSON",
								data : JSON.stringify(datas),
								async : false,
								success : function(data) {
									$("#code").val(data.code);
									var codeSelect1 = $("#first-type-code").val();
									var codeSelect2 = $("#second-type-code").val();
									var codeSelect3 = $("#third-type-code").val();
									var goodsCodeText = codeSelect1 + codeSelect2 + codeSelect3 + data.code;
									$("#goodsCode").text(goodsCodeText);
								},
								error : function(data) {
									AlertErrorMessage("请稍后再试");
								}
							});
						}
					},
					error : function(data) {
						AlertErrorMessage("请稍后再试");
					}
				});
			});
		}
		
		//验证属性 supplyNamesCla
		function lackSupplyParams(){
			var bool = false;
			$(".supplyNamesCla").each(function () {
				var time = $(this).attr("time");
				var supplyNames = $("#supplyNames" + time).val();
				var supplyValues = $("#supplyValues" + time).val();
				
				var isSupplyNamesEmpty = isEmptyString(supplyNames);
				var isSupplyValuesEmpty = isEmptyString(supplyValues);

				if(isSupplyNamesEmpty){
					if(!isSupplyValuesEmpty){
						bool = true;
					}
				}else{
					if(isSupplyValuesEmpty){
						bool = true;
					}
				}
			});
			return bool;
		}
		
		//点击查看大图弹窗
		function watchBigPic(url) {
				var currentProjectURL = $("#currentProjectURL").val();
				OpenSimpleUrl("watch-big-pic", "查看大图", currentProjectURL + "/pageJump?url="+url, '600px', '486px');
			}

	})
	
		
</script>
