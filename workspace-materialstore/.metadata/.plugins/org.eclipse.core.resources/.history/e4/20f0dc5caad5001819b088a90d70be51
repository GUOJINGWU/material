<script>
    layui.use(['form', 'table', 'laypage', 'layer'], function () {
        var laypage = layui.laypage,
                form = layui.form,
                table = layui.table,
                layer = layui.layer;
        
        laypage({
            cont: 'tablePage'
            ,pages: 100 //总页数
            ,groups: 5 //连续显示分页数
          });

    });

    //清空
    function onClear() {
        $(".material-clear").val('');
       
        layui.use('form', function(){
            var form = layui.form(); //只有执行了这一步，部分表单元素才会自动修饰成功
            form.render();
        });

    }
    
    function addMaterial(){
    	var currentProjectURL = $("#currentProjectURL").val();
		OpenSimpleUrl("add-type", "添加分类", currentProjectURL + "/pageJump1", '540px', '416px');
    }

   
    // 模拟tab切换时table对应的物资状态，后台可直接改变，不用以下方法
    $(".audited-tab").click(function(){
        $(".audit-status").html('已通过');
    });
    $(".before-audited-tab").click(function(){
        $(".audit-status").html('待审核');
    });




</script>
